{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": [".next/**", "!.next/cache/**",".medusa/**"]
      },

      "clean": {
        "dependsOn": ["^clean"]
      },
      
      
        "lint": {
            "dependsOn": ["^lint"]
        },
        "test": {
            "dependsOn": ["^test"]
        },
        "dev": {
      "cache": false,
      "persistent": true
    }
      
      
    },
    "globalDependencies": ["turbo.json"]
  }